<h1>編集画面</h1>
<%= form_with(model: @end_user, url: admin_end_user_path(@end_user.id), method: :patch) do |f| %>
    <p>ニックネイム</p>
    <%= f.text_field :nickname%>
    <%= f.text_field :email%>
    <%= f.text_field :bike_type%>
    <%= f.text_field :age%>
    <td>会員ステータス</td>
        <td>
            <%= f.radio_button :is_deleted, true %>
            <%= f.label :is_active, '退会', value: true %>
        </td> 
        <td>
            <%= f.radio_button :is_deleted, false %>
            <%= f.label :is_active, '有効', value: false %>
        </td>
    <%= f.submit '編集を保存' %>
   

<%end%>