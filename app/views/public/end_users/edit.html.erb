<h1><%=@end_user.nickname%>さんの会員編集画面</h1>
<%= form_with(model: @end_user, url:  public_end_user_path(@end_user.id),local:true,method: :patch) do |f| %>
    <p>ニックネイム</p>
    <%= f.text_field :nickname%>
    <p>メールアドレス</p>
    <%= f.text_field :email%>
    <p>プロフィール画像</p>
    <%= f.file_field :profile_image, accept: "image/*" %>
   
    <%= f.submit '変更を保存',class: 'btn-success' %>
    
<%end%>