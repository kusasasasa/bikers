<h1>投稿詳細</h1>
<%=@post.end_user.nickname%>
<%=@post.destination%>
<%=@post.body%>
<%=@post.address%>
<%@post.img_urls.each do |img_url|%>
<%= image_tag img_url, size: "100x100" %>
<%end%>
<%=@age%>代
<% if end_user_signed_in? %>
    <%if @post.end_user_id==current_end_user.id%>
        <%= link_to "投稿を編集", edit_public_post_path(@post.id) %>
    <%end%>
<%end%>
